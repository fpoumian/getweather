{
  "extends": "standard",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "globals": {
    "it": false,
    "expect": false,
    "describe": false,
    "test": false,
    "google": false,
    "jest": false
  }
}